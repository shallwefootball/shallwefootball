<div class="row">
	<div class="col-md-12">
		<div class="panel panel-primary">
				<div class="panel-heading">
					<h3 class="panel-title">참가팀</h3>
				</div>
				<div class="panel-body">
					<% clubs.forEach(function (item, index) { %>
						<% if (index % 3 == 0) { %>
							<div class="col-xs-12">
						<% } %>
								<div class="col-xs-4">
									<a href="javascript:void(0)" data-clubId="<%= item.clubId %>">
										<img class="away center-block" src="/images/team/<%= item.teamId %>/image.jpg" alt="64x64">
										<p class="text-center"><%= item.teamName %></p>
									</a>
								</div>
						<% if (index % 3 == 2 || clubs.length == index) { %>
							</div> <!-- end col-xs-12 -->
						<% } %>
					<% }); %>
							</div> <!-- end col-xs-12 last-->
			</div>
		</div>
	</div>
</div>