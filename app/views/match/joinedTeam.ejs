<div class="row">
	<div class="col-md-12">
		<div class="panel panel-primary">
			<div class="panel-heading">
				<h3 class="panel-title">참가팀</h3>
			</div>
			<div class="panel-body">
				<% clubs.forEach(function (item, index) { %>
					<% if (index % 3 == 0) { %>
						<div class="row">
					<% } %>
							<a href="javascript:void(0)" data-clubId="<%= item.clubId %>">
						        <div class="col-md-4">
									<center>
										<div class="info-item">
						            		<img src="/images/team/<%= item.teamId %>/image.jpg" alt="64x64">
										</div>
										<p class="text-center"><%= item.teamName %></p>
									</center>
									<p></p>
						        </div>
							</a>
					<% if (index % 3 == 2 || ((clubs.length - 1) == index)) { %>
						</div> <!-- end col-xs-12 -->
					<% } %>
				<% }); %>
			</div>
		</div>
	</div>
</div>