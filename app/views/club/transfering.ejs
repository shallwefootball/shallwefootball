<div class="row">
	<div class="col-md-12">

		<table class="table">
			<caption>
				<h5 class="text-left">이적시장. </h5>
			</caption>
			<thead>
				<tr>
					<th></th>
					<th>확인</th>
					<th>이름</th>
					<th>포지션</th>
				</tr>
			</thead>
			<tbody>
				<% club.transfering.forEach(function (item, index) { %>

						<% switch(item.transferStatus) {
							case "transfered" : %>
								<tr class="success">
									<% switch(item.transfer) {
										case "loveCall" : %>
											<td>러브콜</td>
											<td>
												완료 <span class="glyphicon glyphicon-ok text-success"></span>
											</td>
											<td>
												<%= item.playerName %>
											</td>
											<td>
												<%= item.position %>
											</td>
											<td>
												<%= item.squadNumber ? item.squadNumber : "" %>
											</td>
										<% break;
										case "requestCall" : %>
											<td>가입요청</td>
											<td>
												완료 <span class="glyphicon glyphicon-ok text-success"></span>
											</td>
											<td>
												<%= item.playerName %>
											</td>
											<td>
												<%= item.position %>
											</td>
										<% break;
									} %>
								</tr>
							<% break;
							case "wait" : %>
								<tr>
									<% switch(item.transfer) {
										case "loveCall" : %>
											<td>러브콜</td>
											<td>
												대기중..
											</td>
											<td>
												<%= item.playerName %>
											</td>
											<td>
												<%= item.position %>
											</td>
										<% break;
										case "requestCall" : %>
											<td>가입요청</td>
											<td>
												<span class="glyphicon glyphicon-pencil text-primary" data-toggle="modal" data-target=".responseCallModal"></span>
												<!-- <span class="glyphicon glyphicon-remove text-danger" data-toggle="modal" data-target=""></span> -->
											</td>
											<td>
												<%= item.playerName %>
											</td>
											<td>
												<%= item.position %>
											</td>
										<% break;
									} %>
								</tr>
							<% break;
						} %>

				<% }); %>
			</tbody>
