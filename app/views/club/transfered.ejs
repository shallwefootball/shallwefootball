	<div class="col-md-12">
		<h3><span class="text-warning"></span> 새로운 이적생</h3>
		<table class="table">
			<thead>
				<tr>
					<th></th>
					<th>등번호</th>
					<th class="playerName">이름</th>
					<th>등번호수정</th>
					<th>포지션수정</th>
					<th>제외</th>
				</tr>
			</thead>
			<tbody>
				<% club.transfered.forEach(function(player) { %>
					<tr>
						<td>
							<img src="/images/user/<%= player.userId %>" class="img-circle img-responsive" alt="A">
						</td>
						<td class="playerName"><%= player.squadNumber %></td>
						<td>
							<a data-toggle="modal" data-target="#playerLim"><%= player.playerName %></a>
						</td>
						<td>
							<span class="glyphicon glyphicon-pencil text-primary" data-toggle="modal" data-target=".squadNumberModal"
							data-player-name="<%= player.playerName %>"
							data-player-id="<%= player.playerId %>"
							value="<%= player.squadNumber %>"
							></span>
						</td>
						<td>
							<span class="glyphicon glyphicon-pencil text-primary" data-toggle="modal" data-target=".positionModal"
							data-player-name="<%= player.playerName %>"
							data-player-id="<%= player.playerId %>"
							value="<%= player.position %>"
							></span>
						</td>
						<td><span class="glyphicon glyphicon-remove text-danger" data-player-id="<%= player.playerId %>" data-player-name="<%= player.playerName %>"></span></td>
					</tr>
				<% }) %>
			</tbody>
		</table>
	</div>