<table class="table">
	<caption>
		<h4 class="text-left">My Transfer - </h4>
	</caption>
	<thead>
		<tr>
			<th></th>
			<th>확인</th>
			<th>시즌</th>
			<th>클럽</th>
		</tr>
	</thead>
	<tbody>
		<% user.transfer.forEach(function (item, index) { %>
			<% switch(item.transferStatus) {
				case "transfered" : %>
					<tr>
						<% switch(item.transfer) {
							case "loveCall" : %>
								<td>러브콜</td>
								<td>
									<span class="glyphicon glyphicon-ok text-success"></span>
								</td>
								<td>
									<%= item.season %> <%= item.community %>
								</td>
								<td>
									<a href="/league/<%= item.leagueId %>/club/<%= item.clubId %>"><%= item.teamName %></a>
								</td>
							<% break;
							case "requestCall" : %>
								<td>가입요청</td>
								<td>
									<span class="glyphicon glyphicon-ok text-success"></span>
								</td>
								<td>
									<%= item.season %> <%= item.community %>
								</td>
								<td>
									<a href="/league/<%= item.leagueId %>/club/<%= item.clubId %>"><%= item.teamName %></a>
								</td>
							<% break;
						} %>
					</tr>
				<% break;
				case "wait" : %>
					<tr>
						<% switch(item.transfer) {
							case "loveCall" : %>
								<td>러브콜</td>
								<td>
									<span class="glyphicon glyphicon-pencil text-primary" data-toggle="modal" data-target=""></span>
								</td>
								<td>
									<%= item.season %> <%= item.community %>
								</td>
								<td>
									<a href="/league/<%= item.leagueId %>/club/<%= item.clubId %>"><%= item.teamName %></a>
								</td>
							<% break;
							case "requestCall" : %>
								<td>가입요청</td>
								<td>
									대기중..
								</td>
								<td>
									<%= item.season %> <%= item.community %>
								</td>
								<td>
									<a href="/league/<%= item.leagueId %>/club/<%= item.clubId %>"><%= item.teamName %></a>
								</td>
							<% break;
						} %>
					</tr>
				<% break;
				case "reject" :
				case "rejected" : %>
					<tr class="danger">
						<% switch(item.transfer) {
							case "loveCall" : %>
								<td>러브콜</td>
								<td>
									거절
								</td>
								<td>
									<%= item.season %> <%= item.community %>
								</td>
								<td>
									<a href="/league/<%= item.leagueId %>/club/<%= item.clubId %>"><%= item.teamName %></a>
								</td>
							<% break;
							case "requestCall" : %>
								<td>가입요청</td>
								<td>
									거절
								</td>
								<td>
									<%= item.season %> <%= item.community %>
								</td>
								<td>
									<a href="/league/<%= item.leagueId %>/club/<%= item.clubId %>"><%= item.teamName %></a>
								</td>
							<% break;
						} %>
					</tr>
				<% break;
			} %>

		<% }); %>
