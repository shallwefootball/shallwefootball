							var goalScored = false,
								ownGoal = false,
								penaltyScored = false,
								penaltyMissed = false,
								redCard = false,
								yellowCard = false,
								secondYellowCard = false,
								subIn = false,
								subOut = false;
						%>
						<span class="record">
						<% item.records.forEach(function (record, i) { %>
							<% console.log('record   : ', record); %>
							<% switch(record['recordName']) {
								case "goalScored" : %>
									<% if (!goalScored) { %>
										<% goalScored = true; %>
										<img src="/images/record_view-assets/goal.png" alt="">
									<% } %>
									<%= record.minutes %>'
								<% break;
								case "ownGoal" : %>
									<% if (!ownGoal) { %>
										<% ownGoal = true; %>
										<img src="/images/record_view-assets/own-goal.png" alt="">
									<% } %>
									<%= record.minutes %>'&nbsp;
								<% break;
								case "penaltyScored" : %>
									<% if (!penaltyScored) { %>
										<% penaltyScored = true; %>
										<img src="/images/record_view-assets/penalty-goal.png" alt="">
									<% } %>
									<%= record.minutes %>'&nbsp;
								<% break;
								case "penaltyMissed" : %>
									<% if (!penaltyMissed) { %>
										<% penaltyMissed = true; %>
										<img src="/images/record_view-assets/penalty-missed.png" alt="">
									<% } %>
									<%= record.minutes %>'&nbsp;
								<% break;
								case "redCard" : %>
									<% if (!redCard) { %>
										<% redCard = true; %>
										<img src="/images/record_view-assets/red.png" alt="">
									<% } %>
									<%= record.minutes %>'&nbsp;
								<% break;
								case "yellowCard" : %>
									<% if (!yellowCard) { %>
										<% yellowCard = true; %>
										<img src="/images/record_view-assets/yellow.png" alt="">
									<% } %>
									<%= record.minutes %>'&nbsp;
								<% break;
								case "secondYellowCard" : %>
									<% if (!secondYellowCard) { %>
										<% secondYellowCard = true; %>
										<img src="/images/record_view-assets/yellow-red.png" alt="">
									<% } %>
									<%= record.minutes %>'&nbsp;
								<% break;
								case "in" : %>
									<% if (!subIn) { %>
										<% subIn = true; %>
										<img src="/images/record_view-assets/change.png" alt="">
									<% } %>
									<% if (record.time == 'halfTime') { %>
										HT&nbsp;
									<% }else { %>
										<%= record.minutes %>'&nbsp;
									<% } %>
								<% break;
								case "out" : %>
									<% if (!subOut) { %>
										<% subOut = true; %>
										<img src="/images/record_view-assets/change.png" alt="">
									<% } %>
									<% if (record.time == 'halfTime') { %>
										HT&nbsp;
									<% }else { %>
										<%= record.minutes %>'&nbsp;
									<% } %>
								<% break;
							} %>
						<% }); %>
						</span>

                  </div>
                <% }); %>
