		</div>
		<div id="team">
			<div id="home-info">
				<div class="logo">
					<img src="/images/record_view-assets/<%= match.info.homeTeamId %>.png" alt="">
				</div>
				<div class="name">
					<% if (match.info.homeClubName.length < 3) { %>
						<%= match.info.homeClubName[0] %> <%= match.info.homeClubName[1] %>
					<% }else { %>
						<%= match.info.homeClubName %>
					<% } %>
				</div>
				<div class="score"><%= match.info.homeScore %></div>
			</div>
			<div id="away-info">
				<div class="logo">
					<img src="/images/record_view-assets/<%= match.info.awayTeamId %>.png" alt="">
				</div>
				<div class="name">
					<% if (match.info.awayClubName.length < 3) { %>
						<%= match.info.awayClubName[0] %> <%= match.info.awayClubName[1] %>
					<% }else { %>
						<%= match.info.awayClubName %>
					<% } %>
				</div>
				<div class="score"><%= match.info.awayScore %></div>
			</div>
		</div>
		<div id="match_info">
			<span id="year"><%= match.info.year %></span>
			<span id="month"><%= match.info.month %></span>
			<span id="day"><%= match.info.day %></span>
			<span id="stadium"><%= match.info.stadium %></span>
			<span id="weather"><%= match.info.sky %></span>
			<span id="temperature"><%= match.info.temperature %></span>
			<span id="humidity"><%= match.info.humidity %></span>
			<span id="wind"><%= match.info.wind %></span>
		</div>
		<div id="time">
			<span id="first-half-start"><%= match.info.firstHalfHour %> : <%= match.info.firstHalfMinute %></span>
			<!-- <span id="first-half-end">19 : 00</span> -->
			<span id="first-add"><%= match.info.firstHalfAdditional %>'</span>
			<span id="second-half-start"><%= match.info.secondHalfHour %> : <%= match.info.secondHalfMinute %></span>
			<!-- <span id="second-half-end">21 : 29</span> -->
			<span id="second-add"><%= match.info.secondHalfAdditional %>'</span>
		</div>
		<div id="referee">
			<div id="head"><%= match.info.refereeHead[0] %> <%= match.info.refereeHead[1] %> <%= match.info.refereeHead[2] ? match.info.refereeHead[2] : "" %></div>
			<div id="assistants"><%= match.info.refereeAssistant1[0] %> <%= match.info.refereeAssistant1[1] %> <%= match.info.refereeAssistant1[2] ? match.info.refereeAssistant1[2] : "" %>, <%= match.info.refereeAssistant2[0] %> <%= match.info.refereeAssistant2[1] %> <%= match.info.refereeAssistant2[2] ? match.info.refereeAssistant2[2] : "" %></div>
			<div id="fourth"><%= match.info.refereeFourth[0] %> <%= match.info.refereeFourth[1] %> <%= match.info.refereeFourth[2] ? match.info.refereeFourth[2] : "" %></div>
			<div id="fifth"><%= match.info.refereeFourth[0] %> <%= match.info.refereeFourth[1] %> <%= match.info.refereeFourth[2] ? match.info.refereeFourth[2] : "" %></div>
		</div>
		<div id="coach">
			<div id="home-coach"><%= match.info.homeCoach[0] %> <%= match.info.homeCoach[1] %> <%= match.info.homeCoach[2] ? match.info.homeCoach[2] : "" %></div>
			<div id="away-coach"><%= match.info.awayCoach[0] %> <%= match.info.awayCoach[1] %> <%= match.info.awayCoach[2] ? match.info.awayCoach[2] : "" %></div>
		</div>
		<div id="record">
			<div id="home">
                <% match.homePlayers.starting.forEach(function (item, index) { %>
					<div class="player">
						<span class="squad-number"><%= item.squadNumber %></span>
						<% switch(item.matchPosition) {
							case "GK" : %>
								<span class="position">GK</span>
							<% break;
							case "DF" : %>
							    <span class="position">DF</span>
							<% break;
							case "MF" : %>
							    <span class="position">MF</span>
							<% break;
							case "FW" : %>
							    <span class="position">FW</span>
							<% break;
						} %>
						<span class="player-name"><%= item.playerName[0] %> <%= item.playerName[1] %> <%= item.playerName[2] ? item.playerName[2] : "" %></span>

						<%
							var goalScored = false,
								ownGoal = false,
								penaltyScored = false,
								penaltyMissed = false,
								redCard = false,
								yellowCard = false,
								secondYellowCard = false,
								subIn = false,
								subOut = false;
						%>
						<span class="record">
						<% item.records.forEach(function (record, i) { %>
							<% console.log('record   : ', record); %>
							<% switch(record['recordName']) {
								case "goalScored" : %>
									<% if (!goalScored) { %>
										<% goalScored = true; %>
										<img src="/images/record_view-assets/goal.png" alt="">
									<% } %>
									<%= record.minutes %>'
								<% break;
								case "ownGoal" : %>
									<% if (!ownGoal) { %>
										<% ownGoal = true; %>
										<img src="/images/record_view-assets/own-goal.png" alt="">
									<% } %>
									<%= record.minutes %>'&nbsp;
								<% break;
								case "penaltyScored" : %>
									<% if (!penaltyScored) { %>
										<% penaltyScored = true; %>
										<img src="/images/record_view-assets/penalty-goal.png" alt="">
									<% } %>
									<%= record.minutes %>'&nbsp;
								<% break;
								case "penaltyMissed" : %>
									<% if (!penaltyMissed) { %>
										<% penaltyMissed = true; %>
										<img src="/images/record_view-assets/penalty-missed.png" alt="">
									<% } %>
									<%= record.minutes %>'&nbsp;
								<% break;
								case "redCard" : %>
									<% if (!redCard) { %>
										<% redCard = true; %>
